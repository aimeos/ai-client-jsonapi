{
	"name": "aimeos/ai-client-jsonapi",
	"description": "Aimeos frontend JSON REST API",
	"keywords": ["aimeos", "extension", "json", "api"],
	"type": "aimeos-extension",
	"license": "LGPL-3.0-or-later",
	"support": {
		"source": "https://github.com/aimeos/ai-client-jsonapi",
		"issues": "https://github.com/aimeos/ai-client-jsonapi/issues",
		"forum": "https://aimeos.org/help",
		"wiki": "https://aimeos.org/docs"
	},
	"prefer-stable": true,
	"minimum-stability": "dev",
	"require": {
		"php": "~7.1||~8.0",
		"aimeos/aimeos-core": "~2022.04",
		"aimeos/ai-controller-frontend": "~2022.04"
	},
	"require-dev": {
		"phpunit/phpunit": "~7.0||~8.0||~9.0",
		"nyholm/psr7-server": "~1.0"
	},
	"autoload": {
		"psr-4": {
			"Aimeos\\": "src"
		},
		"classmap": [
			"src"
		]
	},
	"autoload-dev": {
		"psr-4": {
			"Aimeos\\": "tests"
		},
		"classmap": [
			"tests"
		]
	}
}